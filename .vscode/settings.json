{
  "[javascript, typescript]": {
    "editor.defaultFormatter": "ms-vscode.vscode-typescript-tslint-plugin",
    "editor.codeActionsOnSave": {
      "source.fixAll.tslint": true
    },
    "eslint.alwaysShowStatus": true,
    "eslint.validate": [
      "javascript",
      "typescript"
    ]
  },
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "files.exclude": {
    "coverage/": true,
  },
  "search.exclude": {
    "**/dist": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/dist": true
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#f8c3ab",
    "activityBar.activeBorder": "#0c9336",
    "activityBar.background": "#f8c3ab",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#0c9336",
    "activityBarBadge.foreground": "#e7e7e7",
    "statusBar.background": "#f4a17c",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#f07f4d",
    "titleBar.activeBackground": "#f4a17c",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#f4a17c99",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#2340c4"
}
